{
  "include": [
    "./src/**/*",
    "../ynap-parsers/src/util/papaparse.ts",
    "../ynap-parsers/src/util/jschardet.d.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["dom", "es2017"],
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "importHelpers": true
  }
}
